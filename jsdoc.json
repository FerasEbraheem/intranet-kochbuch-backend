{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src",
      "app.js",
      "server.js"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs|__tests__|.*\\.test\\.js)"
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ]
}
